<div class="container-fluid">
    <div class="row display-flex">
        <div class="col-sm-1"></div>
        <div class="col-sm-10">
            <ul class="row-fluid block-grid-4">
                {{#each model as |products|}}
                <li>
                    <div class="thumbnail">
                        <img src={{products.image}} alt="Location" style="width:100%; height:275px;">
                        <div class="caption">
                            <p>{{products.name}}</p>
                            <button class="btn btn-default">Add To Cart</button>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ul>
        </div>
        <div class="col-sm-1"></div>
    </div>
</div>


{{#project-footer}}{{/project-footer}}

{{outlet}}
